{
  "name": "simple-web-site",
  "version": "1.0.0",
  "private": false,
  "keywords": [
    "Angular",
    "Playwright",
    "Code Coverage",
    "NestJS"
  ],
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build-docker": "docker build -t simply-web-site:v1.0.0 -f ./Dockerfile .",
    "build:client": "npm run build --prefix client",
    "build:client:coverage": "npm run build:e2e-coverage --prefix client",
    "build:coverage": "npm run build:server && npm run build:client:coverage",
    "build:server": "npm run build --prefix server",
    "commit": "git cz",
    "coverage-report": "npx nyc report",
    "postinstall": "npx husky install",
    "lint": "eslint --fix .",
    "playwright-report": "npx playwright merge-reports --reporter html ./blob-report",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "run-docker": "docker run -p 4200:80 simply-web-site:v1.0.0",
    "start:build": "node build/main",
    "start:client": "npm run start --prefix client",
    "start:server": "npm run start:dev --prefix server",
    "start:server:debug": "npm run start:debug --prefix server",
    "test:karma": "npm run test:karma --prefix client",
    "test:playwright": "npx playwright test"
  },
  "lint-staged": {
    "{client,server}*.{js,ts,css,scss,html,md}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
