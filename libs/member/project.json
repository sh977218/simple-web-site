{
  "name": "member",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/member/src",
  "prefix": "lib",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [
        {
          "dependencies": true,
          "target": "build",
          "params": "forward"
        }
      ],
      "configurations": {
        "production": {
          "tsConfig": "libs/member/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/member/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@angular/build:unit-test",
      "options": {
        "runner": "vitest",
        "tsConfig": "libs/member/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
