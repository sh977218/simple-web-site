{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "client",
  "projects": {
    "simple-web-site": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush"
        }
      },
      "root": "",
      "sourceRoot": "client/simple-web-site/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "build/dist/simple-web-site",
            "index": "client/simple-web-site/src/index.html",
            "browser": "client/simple-web-site/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.angular.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "client/simple-web-site/src/public"
            ],
            "styles": [
              {
                "input": "client/simple-web-site/src/theme.scss"
              },
              "client/simple-web-site/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "client/simple-web-site/src/environments/environment.ts",
                  "with": "client/simple-web-site/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "simple-web-site:build:production"
            },
            "development": {
              "buildTarget": "simple-web-site:build:development"
            },
            "playwright": {
              "buildTarget": "simple-web-site:coverage",
              "liveReload": false,
              "watch": false
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "client/simple-web-site/src/**/*.ts",
              "client/simple-web-site/src/**/*.html"
            ]
          }
        }
      }
    },
    "e2e-coverage": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "client/simple-web-site/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "client/simple-web-site/coverage.webpack.js"
            },
            "outputPath": "build/dist/e2e-coverage",
            "index": "client/simple-web-site/src/index.html",
            "main": "client/simple-web-site/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.angular.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "client/simple-web-site/src/public"
            ],
            "styles": [
              {
                "input": "client/simple-web-site/src/theme.scss"
              },
              "client/simple-web-site/src/styles.scss"
            ],
            "scripts": [],
            "progress": false,
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "client/simple-web-site/src/environments/environment.ts",
                  "with": "client/simple-web-site/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {}
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "buildTarget": "e2e-coverage:build"
          },
          "configurations": {
            "development": {
              "buildTarget": "e2e-coverage:build:development"
            },
            "playwright": {
              "buildTarget": "e2e-coverage:build:production"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ],
    "analytics": false
  }
}
